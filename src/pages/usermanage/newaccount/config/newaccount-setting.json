        "API": "/api/crud/oms/player/players/unlock/(id)",
        "method": "post"
      },
      "expect": {
        "field": "status",
        "value": "FROZEN"
      }
    },
    {
      "title": "冻结账号",
      "type": "request",
      "options": {
        "tips": "确定要冻结账号麽?",
