      "title": "发放GuGu令",
      "type": "modal",
      "options": {
        "style": "primary",
        "modalTitle": "发放GuGu令",
        "modalWidth": 600,
        "items": [{
          "component": "Form",
          "config": {
            "layout": "Grid",
            "layoutConfig": {
              "value": [
                24
              ]
            },
            "API": {
              "getAPI": "/api/crud/oms/playerWallet/playerWallets/self",
              "createAPI": "/api/crud/oms/manager/managers/sendCoins"
            },
            "fields": [{
