                "props": {
                  "style": {
                    "font-size": "24px"
                  }
                },
                "type": "plain",
                "field": "coinsAmount"
              },
              {
                "label": "玩家账号",
                "rules": [{
                  "type": "required",
                  "message": "请设置发放数量"
                }],
                "props": {
                  "placeholder": "玩家账号",
                  "style": {
                    "width": 240
                  }
                },
                "type": "number",
                "field": "targetPlayerMobilePhone"
              },
              {
                "label": "发放数量:",
                "rules": [{
                  "type": "required",
                  "message": "请设置发放数量"
                }],
                "props": {
                  "placeholder": "请输入数量",
                  "style": {
                    "width": 240
                  }
                },
                "type": "number",
                "field": "number"
              },
              {
                "label": "支付密码:",
                "rules": [{
                  "type": "required",
                  "message": "支付密码不能为空"
                }],
                "props": {
                  "placeholder": "请输入支付密码",
                  "style": {
                    "width": 240
                  }
                },
                "type": "password",
                "field": "paymentPassword"
              }
            ]
          }
        }]
      },
      "expect": {
        "permission": "sysTransferGUGU"
      }
    },
    {
      "title": "发放记录",
      "type": "path",
      "options": {
        "path": "/usermanage/systemuser/systemuser-distributionRecord"
      },
      "expect": {
        "permission": "sysTransferRecord"
      }
    }
  ],
